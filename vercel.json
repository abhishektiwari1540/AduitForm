{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "buildCommand": "chmod +x node_modules/.bin/react-scripts && CI=false npm run build",
  "installCommand": "npm install --legacy-peer-deps && chmod +x node_modules/.bin/react-scripts",
  "outputDirectory": "build",
  "framework": "create-react-app"
}